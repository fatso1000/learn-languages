generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider          = "postgresql"
    url               = env("POSTGRES_PRISMA_URL")
    directUrl         = env("POSTGRES_URL_NON_POOLING")
    shadowDatabaseUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
    id         Int      @id @default(autoincrement())
    created_at DateTime @default(now())
    email      String   @unique
    password   String
    name       String?
}
